<h1>Edit article</h1>
<br>
<%= form_for(@article) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.submit %>
<% end %>

 <%= link_to articles_path, class:"btn btn-light" do %>
 <i class="fa-solid fa-house"></i>
 <% end %>
